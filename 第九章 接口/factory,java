package night9;

interface service{
	void a();
}

class implementation1 implements service{
	public void a() {
		System.out.println("method 1");
	}
}

class implementation2 implements service{
	public void a() {
		System.out.println("method 2");
	}
}

public class factory {
	
	public static void make(service s) {              //指定参数为service，在main下就可自由填入service接口的类，向上转型。而无需写多个函数来对应不同的implmentation
		s.a();
	}
	public static void main(String[] args) {
		make(new implementation1());
		make(new implementation2());
	}
}


/*
method 1
method 2
*/
